generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Category {
  id       Int       @id @default(autoincrement())
  name     String    @unique
  seatings Seating[]
}

model Seating {
  id          Int      @id @default(autoincrement())
  description String
  debit       Decimal
  credit      Decimal
  detail      String
  date        DateTime
  numDoc      String
  asn         String
  categoryId  Int
  category    Category @relation(fields: [categoryId], references: [id])
}
